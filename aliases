# My Aliasez
alias chrome='open -a /Applications/Google\ Chrome.app'
alias cls="clear; ls"
alias cp="cp -v"
alias dcr="docker-compose run --rm"
alias dc="docker-compose"
alias dr="docker run --rm -it"
alias di="docker images"
alias dps="docker ps"
alias dk="docker kill"
alias d="docker"
alias drm="docker rm"
alias dcps="docker-compose ps"
alias dm="docker-machine"
alias dmls="docker-machine ls"
alias dmc="docker-machine create"
alias dms="docker-machine start"
alias dot='vim ~/.*'
alias gc='git checkout'
alias gd='git diff'
alias gl='git log'
alias glade='open -a /Applications/Glade.app'
alias gs='git status'
alias lh="clear; ls -larth"
alias ll="ls -alh"
alias ls="ls -G"
alias mux="TERM=screen-256color-bce tmuxinator"
alias ms="mux start"
alias mv="mv -v"
alias oo='open -a /Applications/OpenOffice.org.app'
alias prev='open -a /Applications/Preview.app'
alias rm="rm -v"
alias ta="tmux attach -t"
alias tk="tmux kill-session -t"
alias tl="tmux list-sessions"
alias tlw="tmux list-windows"
alias tmux="TERM=screen-256color-bce tmux"
alias tmuxinator="TERM=screen-256color-bce tmuxinator"
alias ts="mux start"
alias v='vim .'
alias vsh='vagrant ssh'
alias vu='vagrant up'
